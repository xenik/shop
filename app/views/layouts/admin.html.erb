<!DOCTYPE html>
<html>
<head>
  <title>Shop</title>
    <%= stylesheet_link_tag "admin", :media => "all" %>
    <%#= stylesheet_link_tag('public', 'admin', :media => 'all') %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
    <%#= render 'layouts/shim' %>  
</head>
<body>
  <%= render 'layouts/testheader' %>
  <div class="container">
    <% flash.each do |key, value| %>
        <%= content_tag(:div, value, class: "alert alert-#{key}") %>
    <% end %>
    <%= yield %>
    <%= render 'layouts/testfooter' %>
  </div>
  <%= debug(params) if Rails.env.development? %>
  <%= debug(session) if Rails.env.development? %>
  <%#= debug(response) if Rails.env.development? %>
  <%#= params.inspect if Rails.env.development? %>  
  </body>
</html>